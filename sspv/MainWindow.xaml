<Window x:Class="sspv.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:sspv"
        mc:Ignorable="d"
        Title="SSPV" WindowState="Maximized"
        KeyDown="OnKeyDown" Background="Black">
    <Image x:Name="image" Margin="0" />
    <Window.ContextMenu>
        <ContextMenu Name="contextMenu">
            <MenuItem  Header="Strech">
                <MenuItem Header="Fill" Click="OnStrechFill"/>
                <MenuItem Header="None" Click="OnStrechNone"/>
                <MenuItem Header="Uniform" Click="OnStrechUniform"/>
                <MenuItem Header="UniformToFill" Click="OnStrechUniformToFill"/>
            </MenuItem>
        </ContextMenu>
    </Window.ContextMenu>
</Window>
